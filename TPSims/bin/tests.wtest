			var amigo1 = new Sim() amigo1.nivelFelicidad(70) melina.agregarAmigo(amigo1)
			var amigo2 = new Sim() amigo2.nivelFelicidad(55) federico.agregarAmigo(amigo2)
	
	//simQueLeAtrae
